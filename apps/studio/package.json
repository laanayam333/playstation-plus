{
  "name": "studio",
  "private": true,
  "version": "1.0.0",
  "description": "Repository for Feriekoloniens CMS (powered by Sanity.io)",
  "main": "package.json",
  "author": "Rikard Eide <rikardeide@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "sanity start",
    "build": "sanity build public -y",
    "dev": "sanity start",
    "eslint": "eslint --cache .",
    "prettier": "prettier --check . --ignore-path ../../.prettierignore",
    "ts": "tsc",
    "test": "pnpm prettier && pnpm eslint && pnpm ts",
    "backup:prod": "sanity dataset export production backups/$(date \"+%Y-%m-%d\")-prod.tar.gz",
    "backup:dev": "sanity dataset export development backups/$(date \"+%Y-%m-%d\")-dev.tar.gz"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/base": "^2.30.1",
    "@sanity/core": "^2.30.2",
    "@sanity/dashboard": "^2.30.1",
    "@sanity/data-aspects": "^2.29.3",
    "@sanity/default-layout": "^2.30.1",
    "@sanity/default-login": "^2.30.1",
    "@sanity/desk-tool": "^2.30.1",
    "@sanity/eslint-config-studio": "^2.0.0",
    "@sanity/form-builder": "^2.30.1",
    "@sanity/vision": "^2.30.1",
    "eslint": "^8.18.0",
    "prop-types": "^15.8",
    "react": "^18.2",
    "react-dom": "^18.2",
    "react-icons": "^4.4.0",
    "sanity-plugin-media": "^1.4.10",
    "sanity-plugin-vercel-deploy": "^2.1.6",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@sanity/cli": "^2.30.2",
    "config": "workspace:*",
    "tsconfig": "*"
  }
}
